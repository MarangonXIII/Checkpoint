<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TesteActivity">

    <Button
        android:id="@+id/jButtonEsqSenha2"
        android:layout_width="0dp"
        android:layout_height="55dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="41dp"
        android:backgroundTint="#009688"
        android:text="Esqueceu Senha"
        app:layout_constraintBottom_toTopOf="@+id/jUsuario2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/jLogin2"
        app:layout_constraintTop_toBottomOf="@+id/divider2" />

    <Button
        android:id="@+id/jLogin2"
        android:layout_width="0dp"
        android:layout_height="55dp"
        android:layout_marginStart="37dp"
        android:layout_marginEnd="34dp"
        android:backgroundTint="#009688"
        android:text="Login"
        app:layout_constraintBaseline_toBaselineOf="@+id/jButtonEsqSenha2"
        app:layout_constraintEnd_toStartOf="@+id/jButtonEsqSenha2"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/jButtonCadastrar2"
        android:layout_width="0dp"
        android:layout_height="55dp"
        android:layout_marginStart="37dp"
        android:layout_marginEnd="34dp"
        android:backgroundTint="#009688"
        android:text="Cadastrar"
        app:layout_constraintBaseline_toBaselineOf="@+id/jUsuario2"
        app:layout_constraintEnd_toStartOf="@+id/jUsuario2"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/divider2"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginStart="31dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="35dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toTopOf="@+id/jButtonEsqSenha2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/jUsuario2"
        android:layout_width="0dp"
        android:layout_height="55dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="35dp"
        android:backgroundTint="#009688"
        android:text="Usuario"
        app:layout_constraintBottom_toTopOf="@+id/jADM2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/jButtonCadastrar2"
        app:layout_constraintTop_toBottomOf="@+id/jButtonEsqSenha2" />

    <Button
        android:id="@+id/jADM2"
        android:layout_width="0dp"
        android:layout_height="55dp"
        android:layout_marginBottom="460dp"
        android:backgroundTint="#009688"
        android:text="ADM"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/jButtonCadastrar2"
        app:layout_constraintStart_toStartOf="@+id/jButtonCadastrar2"
        app:layout_constraintTop_toBottomOf="@+id/jUsuario2" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="165dp"
        android:layout_height="0dp"
        android:layout_marginTop="47dp"
        android:layout_marginBottom="66dp"
        android:text="Ãrea Teste"
        android:textSize="34sp"
        app:layout_constraintBottom_toTopOf="@+id/divider2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="top" />

</androidx.constraintlayout.widget.ConstraintLayout>